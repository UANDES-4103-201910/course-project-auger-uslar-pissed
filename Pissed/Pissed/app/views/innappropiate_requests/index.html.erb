<p id="notice"><%= notice %></p>

<h1>Innappropiate Requests</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Title</th>
      <th>Justification</th>
      <th>User</th>
      <th>Post</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @innappropiate_requests.each do |innappropiate_request| %>
      <tr>
        <td><%= innappropiate_request.date %></td>
        <td><%= innappropiate_request.title %></td>
        <td><%= innappropiate_request.justification %></td>
        <td><%= innappropiate_request.user %></td>
        <td><%= innappropiate_request.post %></td>
        <td><%= link_to 'Show', innappropiate_request %></td>
        <td><%= link_to 'Edit', edit_innappropiate_request_path(innappropiate_request) %></td>
        <td><%= link_to 'Destroy', innappropiate_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Innappropiate Request', new_innappropiate_request_path %>
